
@{
    ViewData["Title"] = "Shippers";
}

<h1>Shippers</h1>
<button class="btn btn-dark" id="button1">Click</button>

@section Scripts{
    <script>
        const btn = document.querySelector('#button1');
        btn.addEventListener('click', async () => {
            const response = await fetch('https://localhost:7106/api/Shippers/7')
            const datas = await response.json();
            console.log(datas);
        })
       
    </script>
}